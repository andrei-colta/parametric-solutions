{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "/woodworking", "destination": "/woodworking.html" },
      { "source": "/contact", "destination": "/contact.html" },
      { "source": "/laser-engraving", "destination": "/laser-engraving.html" },
      { "source": "/3d-printing", "destination": "/3d-printing.html" },
      { "source": "/3d-printing/**", "destination": "/3d-printing.html" }
    ]
  }
}
